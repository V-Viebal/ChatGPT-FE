						p-tabPanel(
							[header]='searchInput?.value?.length > 0 \
								? ( "LABEL.DEFAULT_PRESET" | translate ) + " (" + tempPresets?.length + ")" \
								: ( "LABEL.DEFAULT_PRESET" | translate ) + " (" + presets?.length + ")"')
